{
  "name": {
    "en": "3D Printer Status"
  },
  "height": 202,
  "settings": [
    {
      "id": "device",
      "type": "autocomplete",
      "title": {
        "en": "Device"
      }
    }
  ],
  "api": {
    "getStatus": {
      "method": "GET",
      "path": "/status"
    },
    "getImage": {
      "method": "GET",
      "path": "/image"
    },
    "setPrintPaused": {
      "method": "POST",
      "path": "/control/pause"
    },
    "setPrintResume": {
      "method": "POST",
      "path": "/control/resume"
    },
    "setPrintStop": {
      "method": "POST",
      "path": "/control/stop"
    },
    "setLightChamber": {
      "method": "POST",
      "path": "/control/light_chamber"
    },
    "setPrintSpeed": {
      "method": "POST",
      "path": "/control/print_speed"
    },
    "setFanSpeed": {
      "method": "POST",
      "path": "/control/fan_speed"
    }
  }
